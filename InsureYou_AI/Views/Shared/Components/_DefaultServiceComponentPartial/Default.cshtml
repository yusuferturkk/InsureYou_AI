@model List<Service>

<section id="services-05" class="services-05 services-09 pt-240 pb-100 p-relative fix" style=" background-color: #28314b;">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-12 col-md-12">
                <div class="section-title center-align mb-50 text-center" data-aos="fade-up" data-aos-duration="1000">
                    <h2>
                        Sunmuş Olduğumuz Sigorta Hizmetleri
                    </h2>
                </div>
            </div>
            <div class="col-lg-12">
                <div class="services-active" data-aos="fade-up" data-aos-duration="1000">
                    @foreach (var item in Model)
                    {
                        <div class="services-box-02">
                            <div class="services-img">
                                <img src="@item.ImageUrl" alt="icon01" style="height:400px">
                                <div class="services-content2">
                                    <div class="icon"><img src="@item.IconUrl" alt="icon01"></div>
                                    <h4><a href="single-service.html">@item.Title</a></h4>
                                    <p>@item.Description</p>
                                    <a class="link" href="single-service.html"><i class="fal fa-angle-right"></i></a>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
        <div class="row align-items-center">
            <div class="col-md-12 text-center">
                <div class="text-bg" data-aos="fade-up" data-aos-duration="1000">
                    <h2>Sayılarla Biz</h2>
                </div>
            </div>
        </div>
    </div>
</section>